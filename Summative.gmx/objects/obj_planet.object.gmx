            <string>randomize();

enum MATERIAL {

    ICE = 1,
    ROCK = 2,
    COPPER = 3,
    GOLD = 4,
    PLATINUM = 5,
    DIAMOND = 6

};

//Random size
radius = random_range(40, 200);

//Random material based on score
if(score &lt; 10) {

    material = choose(MATERIAL.COPPER, MATERIAL.COPPER, MATERIAL.GOLD);

} else if(score &lt; 20) {

    material = choose(MATERIAL.ROCK, MATERIAL.COPPER, MATERIAL.COPPER, MATERIAL.GOLD);

} else if(score &lt; 30) {

    material = choose(MATERIAL.ROCK, MATERIAL.ROCK, MATERIAL.COPPER, MATERIAL.COPPER, MATERIAL.GOLD, MATERIAL.PLATINUM);

} else if(score &lt; 40) {

    material = choose(MATERIAL.ICE, MATERIAL.ROCK, MATERIAL.COPPER, MATERIAL.GOLD, MATERIAL.PLATINUM);
    
} else {

    choose(MATERIAL.ICE, MATERIAL.ROCK, MATERIAL.COPPER, MATERIAL.GOLD, MATERIAL.PLATINUM, MATERIAL.DIAMOND);
    
}
   
//Set the colour based on material 
switch (material) {
    case MATERIAL.ICE:
        colour = make_colour_rgb(209, 249, 246);
        break;
    case MATERIAL.ROCK:
        colour = make_colour_rgb(84, 75, 53);
        break;
    case MATERIAL.COPPER:
        colour = make_colour_rgb(119, 68, 51);
        break;
    case MATERIAL.GOLD:
        colour = make_colour_rgb(255,215,0);
        break;
    case MATERIAL.PLATINUM:
        colour = make_colour_rgb(229, 228, 226);
        break;
    case MATERIAL.DIAMOND:
        colour = make_colour_rgb(185,242,255);
        break;
    default: break;
}

if(score != 0) {

    for(i = 0; i &lt; random_range(0, score + 1 / 10 * radius / 60); i++) {
    
        dir[i] = random_range(0, 359);
    
        for(j = 0; j &lt; random_range(0, 2); j++) {
        
            spike[i * 3 + j] = instance_create(x + radius * cos(degtorad(dir[i] + j * 20)), y + radius * sin(degtorad(dir[i] + j * 20)), obj_spike);
            spike[i * 3 + j].image_angle = point_direction(x, y, spike[i * 3 + j].x, spike[i * 3 + j].y) - 90;
            
        }
    
    }
    
}

if(random_range(0, 1) &amp;&amp; score != 0) {

    beetle = instance_create(x + radius * cos(degtorad(0)), y + radius * sin(degtorad(0)), obj_beetle);
    beetle.planet = self;
    
}
</string>
